<%- include('../../partials/header.ejs') %>
<%- include('../../partials/homenavbar.ejs') %>
<body>
  <p><div class="container">
  <% result.articles.rows.forEach(article => { %>
      <p><div class="card">
        <div class="card-header">
          <h2><%= article.title %></h2>          
        </div>
        <div class="card-body">
          <a href="/<%= article.slug %>" class="btn btn-success">Read article</a>
        </div>
      </div></p>
    <% }); %> 
  </div></p>
</body>
